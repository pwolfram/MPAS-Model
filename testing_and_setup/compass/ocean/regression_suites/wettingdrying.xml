<regression_suite name="nightly_ocean_test_suite">
	<test name="Overflow 10km" core="ocean" configuration="overflow" resolution="10km" test="default">
		<script name="run_test.py"/>
	</test>
	<test name="No drying due to wave on slope 10km" core="ocean" configuration="onshore_wave" resolution="10km" test="default">
		<script name="run_test.py"/>
	</test>
	<test name="Drying due to wave on slope 10km" core="ocean" configuration="onshore_wave" resolution="10km" test="drying">
		<script name="run_test.py"/>
	</test>
</regression_suite>
